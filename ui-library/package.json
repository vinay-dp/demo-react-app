{
  "name": "ui-library",
  "private": false,
  "version": "1.1.1",
  "description": "A simple React component library with Button, Icon, Input, and Dropdown components",
  "keywords": [
    "react",
    "components",
    "ui-library",
    "typescript"
  ],
  "author": "Vinay Panchal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinay-dp/demo-react-app.git",
    "directory": "ui-library"
  },
  "type": "module",
  "main": "./dist/ui-library.umd.js",
  "module": "./dist/ui-library.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ui-library.es.js",
      "require": "./dist/ui-library.umd.js"
    },
    "./dist/ui-library.css": "./dist/ui-library.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "prepare": "cd .. && husky ui-library/.husky",
    "commit": "cz",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "standard-version": "^9.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  }
}
